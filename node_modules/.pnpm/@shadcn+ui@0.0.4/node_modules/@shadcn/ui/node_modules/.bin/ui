#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules/@shadcn/ui/dist/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules/@shadcn/ui/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules/@shadcn/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules/@shadcn/ui/dist/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules/@shadcn/ui/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules/@shadcn/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/@shadcn+ui@0.0.4/node_modules:/Users/maxime/Documents/Code/mh_wilds_talents_translator/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/index.js" "$@"
else
  exec node  "$basedir/../../dist/index.js" "$@"
fi
